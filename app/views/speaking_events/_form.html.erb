<%= simple_form_for(@speaking_event) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date %>
    <%= f.input :time %>
    <%= f.input :street_address %>
    <%= f.input :city %>
    <%= f.input :zipcode %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.association :speaker %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
