<%= simple_form_for(@other_event) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :date %>
    <%= f.input :time %>
    <%= f.input :street_address %>
    <%= f.input :city %>
    <%= f.input :zipcode %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
