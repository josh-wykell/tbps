<p id="notice"><%= notice %></p>

<h1>Listing Other Events</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Date</th>
      <th>Time</th>
      <th>Descritption</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @other_events.each do |other_event| %>
      <tr>
        <td><%= other_event.title %></td>
        <td><%= other_event.date %></td>
        <td><%= other_event.time %></td>
        <td><%= other_event.descritption %></td>
        <td><%= link_to 'Show', other_event %></td>
    <% end %>
  </tbody>
</table>

<br>

