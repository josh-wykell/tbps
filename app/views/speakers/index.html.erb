<p id="notice"><%= notice %></p>

<h1>Listing Speakers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
      <th>Time</th>
      <th>Url</th>
      <th>Image</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @speakers.each do |speaker| %>
      <tr>
        <td><%= link_to "#{speaker.name}", speaker_path(speaker) %></td>
        <td><%= speaker.date %></td>
        <td><%= speaker.time %></td>
        <td><%= speaker.url %></td>
        <td><%= speaker.image %></td>
        <td><%= speaker.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

